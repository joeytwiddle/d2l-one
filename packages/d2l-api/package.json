{
  "name": "d2l-api-gql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "yarn install && yarn run codegen",
    "start": "node .",
    "watch": "nodemon -e '*' -w src -w index.js -w schema.gql -d 1 -x 'yarn start' || exit 1",
    "precodegen": "cp -af ../d2l-expo/graphql.tsx ./graphql.last.tsx || true",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "( diff --color=always graphql.last.tsx ../d2l-expo/graphql.tsx || true )",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "googleapis": "39",
    "graphql": "^16.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.2.2",
    "nodemon": "^2.0.15"
  }
}
